<h2>会員一覧</h2>
<table class="admin_customer_index">
  <thead>
    <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @customers.each do |customer| %>
    <tr>
      <td><%= customer.id %></td>
      <td><%= "#{customer.family_name}}" %></td>
      
    </tr>
  </tbody>
</table>